  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Ashi-Ashish/FleetTrace/issues"
  },
  "homepage": "https://github.com/Ashi-Ashish/FleetTrace#readme",
  "devDependencies": {
    "ajv": "^8.17.1",
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^3.0.1"
  }
}
